<% include ./partials/header %>

<h1>Add New Post</h1>

<form action="/story/create" method="POST">
  <div>
    <input type="text" name="title" placeholder="TITLE">
  </div>
  <div>
    <input type="text" name="image" placeholder="Image URL">
  </div>
  <div>
    <textarea name="body"></textarea>
  </div>
  <div>
    <button>Submit</button>
  </div>
</form>

